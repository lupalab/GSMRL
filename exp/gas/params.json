{
    "gpu": "3",
    "seed": 1234,
    "dataset": "vec",
    "dfile": "./data/gas.pkl",
    "dimension": 10,
    "batch_size": 1024,
    "model": "acflow",
    "lambda_mse": 10.0,
    "transform": ["AF","TL","TL","TL","TL","AF"],
    "layer_cfg": ["ML","LR","CP2"],
    "rnncp_units": 256,
    "rnncp_layers": 2,
    "linear_hids": [256,256],
    "linear_rank": -1,
    "affine_hids": [256,256],
    "coupling_hids": [256,256],
    "prior": "autoreg",
    "n_components": 40,
    "prior_units": 256,
    "prior_layers": 2,
    "prior_hids": [256,256],
    "epochs": 500,
    "optimizer": "adam",
    "lr": 0.001,
    "decay_steps": 10000,
    "decay_rate": 0.5,
    "clip_gradient": 1,
    "exp_dir": "./exp/gas",
    "summ_freq": 100,
    "num_samples": 50
}
